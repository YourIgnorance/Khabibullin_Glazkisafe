<Window x:Class="Khabibullin_Glazkisafe.SelfWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Khabibullin_Glazkisafe"
        mc:Ignorable="d"
        Title="SelfWindow" Height="200" Width="300">
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Bottom">
            <TextBlock Text="Изменить приоритет на:"></TextBlock>
            <TextBox Name="TBPriority" FontSize="15" Height="30" Width="150"></TextBox>
            <Button Content="Задать приоритет" Name="EditPriorityButton"   Click="EditPriorityButton_Click" FontSize="25" Height="35" Width="200" HorizontalAlignment="Center" VerticalAlignment="Bottom" Grid.Row="1"></Button>
        </StackPanel>
    </Grid>
</Window>
